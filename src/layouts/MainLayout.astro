---
//component import
import MainHead from "./MainHead.astro"
import Nav from "../components/Nav.astro"
import Footer from "../components/Footer.astro"
import TabBar from "../components/TabBar.astro"

//style import
import "../input.css"

interface Props{
    title: string;
    description: string
}
const { title = "Liu App", description = "default post decription"} = Astro.props
---

<html lang="en">
	<MainHead {title} {description} />
	<body>
        <Nav />
		<slot />  
	</body>
    <TabBar />
    <Footer />
</html>